<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.metodmtsk">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:name=".Util.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitiy.AfterSplash"></activity>
        <activity android:name=".Activitiy.SignDetailActivity" />
        <activity android:name=".Activitiy.SignActivity" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity android:name=".Activitiy.AppointmentResultActivity" />
        <activity android:name=".Activitiy.PopupCenterActivity" />
        <activity android:name=".Activitiy.AppintmentSignActivity" />
        <activity android:name=".Activitiy.VideoMainActivity" />
        <activity android:name=".Activitiy.VideoDetailActivity" />
        <activity android:name=".Activitiy.TestDetailActivity" />
        <activity android:name=".Activitiy.TestActivity" />
        <activity android:name=".Activitiy.NotificationActivity" />
        <activity android:name=".Activitiy.VideoActivity" />
        <activity android:name=".Activitiy.AppointmentActivity" />
        <activity android:name=".Activitiy.LoginActivity" />
        <activity
            android:name=".Activitiy.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Activitiy.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>